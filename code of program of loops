// 1st program
// Online C compiler to run C program online
#include <stdio.h>
// n=5 1 2 3 4 5
// intialization condition operator 
int main() {
  int i=1,n; //intialization
  printf("enter your nmber \n");
  scanf("%d",&n);
  while(i<=n){  //condition
      printf("%d\t",i);
      i++; // operator
  }
 

    return 0;
}

// 2nd program 
// Online C compiler to run C program online
#include <stdio.h>
// n=5 5 4 3 2 1
// intialization condition operator 
int main() {
  int i,n; //intialization
  printf("enter your nmber \n");
  scanf("%d",&n);
    i=n; // intiialize
    do{
        printf("%d\t",i); 
        i--; //oprator
    }while(i>=1); // condtion
 

    return 0;
}
//3rd program 
// Online C compiler to run C program online
#include <stdio.h>
// n=5 2 4
int main() {
  int i=2,n; //intialization
  printf("enter your nmber \n");
  scanf("%d",&n);
  while(i<=n){
      printf("%d\t",i);
     i= i+2;
  }
    return 0;
}

